<h1>Books</h1>
<table>
	<thead>
		<tr>
			<th>Title</th>
			<th>Body</th>
			<th colspan="3"></th>
		</tr>
		<tbody>
			<tr>
				<td>dddddddddd</td>
				<td>eeeeeeeeeeeeee</td>
			</tr>
		</tbody>
	</thead>
</table>

<h2>New book</h2>
<%= form_for(@book) do |f| %>
	<h4>Title</h4>
	<%= f.text_field :title %>
	<h4>Body</h4>
	<%= f.text_area :body %>
	<%= f.submit "Create Book" %>
<% end %>